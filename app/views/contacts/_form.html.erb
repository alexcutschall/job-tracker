<%= form_for [@company, @contact] do |f| %>
  <div class="form-group">
    <%= f.label :full_name %>
    <%= f.text_field :full_name %>
  </div>

  <div class="form-group">
    <%= f.label :role %>
    <%= f.text_field :position %>
  </div>

  <div class="form-group">
    <%= f.label :email %>
    <%= f.text_field :email %>
  </div>
  
  <div class="form-group">
    <button type="submit" class="btn btn-primary">Submit</button>
  </div>
<% end %>
